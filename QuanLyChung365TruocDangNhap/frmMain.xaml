                                                                        <TextBlock
                                                                            Margin="0,0,5,0"
                                                                            FontSize="15"
                                                                            FontWeight="DemiBold"
                                                                            Text="Mật khẩu" />
                                                                        <TextBlock
                                                                            FontSize="15"
                                                                            Foreground="#ff3333"
                                                                            Text="*" />
                                                                    </DockPanel>
                                                                    <Border
                                                                        Margin="0,5,0,0"
                                                                        BorderBrush="#aaa"
                                                                        BorderThickness="1"
                                                                        CornerRadius="5">
                                                                        <Grid Height="37">
                                                                            <Grid.ColumnDefinitions>
                                                                                <ColumnDefinition Width="37*" />
                                                                                <ColumnDefinition Width="312*" />
                                                                                <ColumnDefinition Width="0*" />
                                                                            </Grid.ColumnDefinitions>
                                                                            <PasswordBox
                                                                                x:Name="tb_MatKhauGo"
                                                                                Grid.ColumnSpan="3"
                                                                                Height="40"
                                                                                Margin="1,7,-3,-2"
                                                                                Padding="3,3,0,0"
                                                                                VerticalAlignment="Center"
                                                                                Background="Transparent"
                                                                                BorderThickness="0"
                                                                                FontSize="15"
                                                                                Foreground="#474747"
                                                                                PasswordChanged="tb_MatKhauGo_PasswordChanged" />
                                                                            <TextBlock
                                                                                x:Name="txtMK"
                                                                                Grid.ColumnSpan="3"
                                                                                Margin="8,0,-10,11"
                                                                                HorizontalAlignment="Left"
                                                                                VerticalAlignment="Bottom"
                                                                                FontSize="15"
                                                                                Foreground="#666666"
                                                                                IsHitTestVisible="False"
                                                                                Text="Nhập mật khẩu"
                                                                                TextWrapping="Wrap">
                                                                                <TextBlock.Style>
                                                                                    <Style TargetType="TextBlock">
                                                                                        <Setter Property="Visibility" Value="Collapsed" />
                                                                                        <Style.Triggers>
                                                                                            <DataTrigger Binding="{Binding ElementName=tb_MatKhauGo, Path=Password}" Value="">
                                                                                                <Setter Property="Visibility" Value="Visible" />
                                                                                            </DataTrigger>
                                                                                        </Style.Triggers>
                                                                                    </Style>
                                                                                </TextBlock.Style>
                                                                            </TextBlock>
                                                                        </Grid>
                                                                    </Border>

                                                                </StackPanel>
                                                                <DockPanel>
                                                                    <CheckBox
                                                                        x:Name="chkLuuTKMK"
                                                                        Margin="50,10,0,0"
                                                                        Content="Lưu tài khoản mật khẩu"
                                                                        FontSize="16"
                                                                        Foreground="#474747"
                                                                        IsChecked="True"
                                                                        Style="{DynamicResource checkBox1}" />
                                                                    <TextBlock
                                                                        Margin="0,10,50,0"
                                                                        HorizontalAlignment="Right"
                                                                        Cursor="Hand"
                                                                        FontSize="15"
                                                                        FontWeight="DemiBold"
                                                                        Foreground="#4c5bd4"
                                                                        Text="Quên mật khẩu?" />

                                                                </DockPanel>
                                                                <Border
                                                                    x:Name="btnDangNhapGo"
                                                                    Height="34"
                                                                    Margin="50,15,50,0"
                                                                    Background="#4c5bd4"
                                                                    CornerRadius="20"
                                                                    Cursor="Hand"
                                                                    MouseLeftButtonUp="btnDangNhapGo_MouseLeftButtonUp">
                                                                    <TextBlock
                                                                        Margin="20,0,0,0"
                                                                        HorizontalAlignment="Center"
                                                                        VerticalAlignment="Center"
                                                                        FontSize="16"
                                                                        Foreground="#ffffff"
                                                                        Text="Đăng nhập" />
                                                                </Border>
                                                                <DockPanel Margin="0,15,0,0" HorizontalAlignment="Center">
                                                                    <TextBlock
                                                                        HorizontalAlignment="Right"
                                                                        FontSize="15"
                                                                        FontWeight="DemiBold"
                                                                        Foreground="#474747"
                                                                        Text="Bạn chưa có tài khoản?" />
                                                                    <Border
                                                                        x:Name="btnDangKyGo"
                                                                        Margin="5,0,0,0"
                                                                        HorizontalAlignment="Right"
                                                                        Background="Transparent"
                                                                        Cursor="Hand"
                                                                        MouseLeftButtonUp="btnDangKyGo_MouseLeftButtonUp">
                                                                        <TextBlock
                                                                            FontSize="15"
                                                                            FontWeight="DemiBold"
                                                                            Foreground="#Ffa800"
                                                                            Text="Đăng ký ngay" />
                                                                    </Border>

                                                                </DockPanel>
                                                            </StackPanel>
                                                        </Border>
                                                    </Grid>
                                                </Border>

                                            </Grid>
                                        </Grid>
                                    </Border>
                                </Grid>
                            </Grid>
                        </Grid>
                    </ScrollViewer>
                </Grid>
            </Grid>
            <Border
                x:Name="btnDisplayFullSizebar"
                Width="44"
                Height="44"
                Margin="60,0,0,100"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Background="#616FDF"
                CornerRadius="1000"
                MouseLeftButtonUp="btnDisplayFullSizebar_MouseLeftButtonUp"
                Visibility="Collapsed">
                <Path
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Data="{DynamicResource iconRight2}"
                    Stroke="#ffffff"
                    StrokeThickness="2" />
            </Border>
            <Border
                x:Name="btnCollapseSizebar"
                Width="44"
                Height="44"
                Margin="328,0,0,100"
                HorizontalAlignment="Left"
                VerticalAlignment="Bottom"
                Background="#616FDF"
                CornerRadius="1000"
                MouseLeftButtonUp="btnCollapseSizebar_MouseLeftButtonUp"
                Visibility="Collapsed">
                <Path
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    Data="{DynamicResource iconLeft2}"
                    Stroke="#ffffff"
                    StrokeThickness="2" />
            </Border>
            <Grid
                x:Name="clearPopUp"
                Background="Transparent"
                MouseLeftButtonUp="clearPopUp_MouseLeftButtonUp"
                Opacity="0.5"
                Visibility="{Binding ElementName=popup, Path=Visibility}" />
            <Grid x:Name="popup" Visibility="Collapsed" />
            <Grid
                x:Name="borThongTinChiTiet"
                Width="230"
                Margin="0,125,38,0"
                HorizontalAlignment="Right"
                VerticalAlignment="Top"
                Background="Transparent"
                Visibility="Collapsed">
                <Border
                    x:Name="popupMoreMessageConversation"
                    HorizontalAlignment="Right"
                    VerticalAlignment="Top"
                    Background="#ffffff"
                    CornerRadius="10">
                    <Border.Effect>
                        <DropShadowEffect
                            BlurRadius="10"
                            Opacity="0.4"
                            ShadowDepth="0" />
                    </Border.Effect>
                    <StackPanel>
                        <Border
                            x:Name="btnThongTinTK"
                            Width="230"
                            Height="40"
                            Background="Transparent"
                            CornerRadius="15,15,0,0"
                            Cursor="Hand"
                            MouseLeftButtonUp="btnThongTinTK_MouseLeftButtonUp">
                            <Border.Resources>
                                <Style TargetType="{x:Type Border}">
                                    <Setter Property="Background" Value="#0000" />
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="{DynamicResource colorMoveMouseItem}" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Resources>
                            <DockPanel Margin="12,0,0,0">
                                <Border Width="17" Height="17">
                                    <Path
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Data="{DynamicResource iconAccount2}"
                                        Fill="#474747"
                                        Stretch="Fill" />
                                </Border>
                                <TextBlock
                                    Margin="16,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="14"
                                    Foreground="#474747"
                                    Text="Thông tin tài khoản" />

                            </DockPanel>
                        </Border>
                        <Border
                            x:Name="btnDanhGia"
                            Width="230"
                            Height="40"
                            Cursor="Hand"
                            MouseLeftButtonUp="btnDanhGia_MouseLeftButtonUp">
                            <Border.Resources>
                                <Style TargetType="{x:Type Border}">
                                    <Setter Property="Background" Value="#0000" />
                                    <Style.Triggers>
                                        <Trigger Property="IsMouseOver" Value="True">
                                            <Setter Property="Background" Value="{DynamicResource colorMoveMouseItem}" />
                                        </Trigger>
                                    </Style.Triggers>
                                </Style>
                            </Border.Resources>
                            <DockPanel Margin="10,0,0,0">
                                <DockPanel>
                                    <Path
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Data="{DynamicResource iconChinhSua}"
                                        Fill="#474747" />
                                </DockPanel>
                                <TextBlock
                                    Margin="16,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="14"
                                    Foreground="#474747"
                                    Text="Đánh giá" />

                            </DockPanel>
                        </Border>
                        <Border
                            x:Name="btnBaoLoi"
                            Width="230"
                            Height="40"
                            Cursor="Hand"
                            MouseLeftButtonUp="btnBaoLoi_MouseLeftButtonUp">
                            <DockPanel Margin="10,0,0,0">
                                <Border Width="17" Height="17">
                                    <Path
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Data="{DynamicResource iconWarning}"
                                        Stretch="Fill"
                                        Stroke="#474747" />
                                </Border>
                                <TextBlock
                                    Margin="16,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="14"
                                    Foreground="#474747"
                                    Text="Báo lỗi" />

                            </DockPanel>
                        </Border>
                        <Border
                            x:Name="LogOut"
                            Width="230"
                            Height="40"
                            Background="Transparent"
                            Cursor="Hand"
                            MouseLeftButtonUp="LogOut_MouseLeftButtonUp">
                            <DockPanel Margin="12,0,0,0">
                                <Border Width="17" Height="17">
                                    <Path
                                        HorizontalAlignment="Center"
                                        VerticalAlignment="Center"
                                        Data="{DynamicResource iconLogout}"
                                        Stretch="Fill"
                                        Stroke="#474747"
                                        StrokeThickness="2" />

                                </Border>
                                <TextBlock
                                    Margin="10,0,0,0"
                                    HorizontalAlignment="Left"
                                    VerticalAlignment="Center"
                                    FontSize="14"
                                    Foreground="#474747"
                                    Text="Đăng xuất" />

                            </DockPanel>
                        </Border>
                    </StackPanel>
                </Border>
            </Grid>
        </Grid>
    </Grid>
</Window>
