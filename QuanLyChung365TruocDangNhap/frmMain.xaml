<Window
    x:Class="QuanLyChung365TruocDangNhap.frmMain"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:QuanLyChung365TruocDangNhap"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="frmMains"
    Title="frmMain"
    MinWidth="1000"
    KeyUp="Window_KeyUp"
    SizeChanged="Window_SizeChanged"
    WindowStartupLocation="CenterScreen"
    WindowStyle="None"
    mc:Ignorable="d">
    <Grid>
        <Grid x:Name="pnlShowPopUp">
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="35" />
                    <RowDefinition />
                </Grid.RowDefinitions>
                <Grid
                    Grid.Row="0"
                    Height="30"
                    Background=" #FFFFFF">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <Grid
                        x:Name="pnlTieuDe1"
                        Grid.Row="0"
                        Grid.Column="0"
                        Background="{DynamicResource #FFFFFF}"
                        MouseLeftButtonDown="pnlTieuDe1_MouseLeftButtonDown">
                        <Path
                            Margin="5,0,0,0"
                            VerticalAlignment="Center"
                            Data="{DynamicResource logoTimViec}"
                            Fill="{DynamicResource #4C5BD4}" />
                    </Grid>
                    <DockPanel
                        Grid.Column="1"
                        Margin="0,0,5,0"
                        HorizontalAlignment="Right">
                        <Border
                            x:Name="btnMinimize"
                            Width="25"
                            Background="{DynamicResource #FFFFFF}"
                            MouseLeftButtonUp="btnMinimize_MouseLeftButtonUp">
                            <Path
                                VerticalAlignment="Center"
                                Data="{DynamicResource iconMiniMize}"
                                Stroke="{DynamicResource #666666}" />
                        </Border>
                        <Border
                            x:Name="btnMaximize"
                            Width="25"
                            Margin="0,0,5,0"
                            Background="{DynamicResource #FFFFFF}"
                            MouseLeftButtonUp="btnMaximize_MouseLeftButtonUp">
                            <Path
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Data="{DynamicResource iconMaximize}"
                                Stroke="{DynamicResource #666666}" />
                        </Border>
                        <Border
                            x:Name="btnNomal"
                            Width="25"
                            Margin="0,0,5,0"
                            VerticalAlignment="Center"
                            Background="{DynamicResource #FFFFFF}"
                            MouseLeftButtonUp="btnNomal_MouseLeftButtonUp"
                            Visibility="Collapsed">
                            <Path Data="{DynamicResource iconMaximize1}" Fill="{DynamicResource #666666}" />
                        </Border>
                        <Border
                            x:Name="btnClose"
                            Width="25"
                            Margin="0,0,5,0"
                            Background="{DynamicResource #FFFFFF}"
                            MouseLeftButtonUp="btnClose_MouseLeftButtonUp">
                            <Path
                                HorizontalAlignment="Center"
                                VerticalAlignment="Center"
                                Data="{DynamicResource iconCloseProject}"
                                Fill="{DynamicResource #666666}" />
                        </Border>
                    </DockPanel>
                </Grid>
                <Grid Grid.Row="1">
                    <ScrollViewer x:Name="ScrollMain">
                        <Grid>
                            <DockPanel
                                x:Name="pnlMain"
                                Background="#F5F7FF"
                                Visibility="Visible">
                                <StackPanel
                                    x:Name="pnlFullSizebar"
                                    Grid.Column="0"
                                    Width="348"
                                    Background="#4c5bd4">
                                    <Border
                                        x:Name="TrangChuQuanLyChung"
                                        Height="87"
                                        MouseLeftButtonUp="TrangChuQuanLyChung_MouseLeftButtonUp">
                                        <Image
                                            Width="140"
                                            Height="43"
                                            Source="/Resource/image/Group 632585.png" />
                                    </Border>
                                    <Border
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="#4AA7FF"
                                        CornerRadius="10"
                                        Visibility="Collapsed">
                                        <DockPanel
                                            Margin="15,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path Data="{DynamicResource iconUd}" Fill="#FFFFFF" />
                                            <TextBlock
                                                Margin="10,0,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#FFFFFF"
                                                Text="Ứng dụng" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="bod_CoCau_ViTri_ToChuc"
                                        Height="auto"
                                        Margin="15,20,15,0"
                                        Background="Transparent"
                                        CornerRadius="10">
                                        <StackPanel>
                                            <DockPanel
                                                x:Name="btn_ThietLap"
                                                Width="300"
                                                Height="41"
                                                Margin="12,0,0,0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Background="Transparent"
                                                Cursor="Hand"
                                                MouseLeftButtonUp="btn_ThietLap_MouseLeftButtonUp">
                                                <Path Data="{DynamicResource iconQLCongTy}" Fill="#ffffff" />
                                                <TextBlock
                                                    x:Name="tb_ThietLapCongTy"
                                                    Margin="10,5,0,0"
                                                    FontSize="16"
                                                    FontWeight="DemiBold"
                                                    Foreground="#ffffff"
                                                    Text="Thiết lập công ty" />
                                                <Grid>
                                                    <Path
                                                        x:Name="iconControlBottom"
                                                        HorizontalAlignment="Right"
                                                        VerticalAlignment="Center"
                                                        Data="{DynamicResource iconControlBottomNV}"
                                                        Stroke="#FFFFFF"
                                                        StrokeThickness="1.5" />
                                                    <Path
                                                        x:Name="iconControlTop"
                                                        HorizontalAlignment="Right"
                                                        VerticalAlignment="Center"
                                                        Data="{DynamicResource iconControlBottomNV}"
                                                        RenderTransformOrigin="0.5,0.5"
                                                        Stroke="#FFFFFF"
                                                        StrokeThickness="1.5"
                                                        Visibility="Collapsed">
                                                        <Path.RenderTransform>
                                                            <TransformGroup>
                                                                <ScaleTransform />
                                                                <SkewTransform />
                                                                <RotateTransform Angle="179.83" />
                                                                <TranslateTransform />
                                                            </TransformGroup>
                                                        </Path.RenderTransform>
                                                    </Path>
                                                </Grid>
                                            </DockPanel>
                                            <StackPanel
                                                x:Name="stp_ListSoDo"
                                                Margin="20,5,0,0"
                                                Visibility="Collapsed">
                                                <Border
                                                    x:Name="SoDoToChuc"
                                                    Background="Transparent"
                                                    Cursor="Hand"
                                                    MouseLeftButtonUp="SoDoToChuc_MouseLeftButtonUp">
                                                    <DockPanel>
                                                        <Path
                                                            VerticalAlignment="Center"
                                                            Data="{DynamicResource iocnGach}"
                                                            Stroke="#FFFFFF"
                                                            StrokeThickness="2" />
                                                        <TextBlock
                                                            Margin="10,0,0,0"
                                                            VerticalAlignment="Center"
                                                            FontSize="15"
                                                            FontWeight="Regular"
                                                            Foreground="#FFFFFF"
                                                            Text="Thiết lập cơ cấu tổ chức" />
                                                    </DockPanel>
                                                </Border>
                                                <Border
                                                    x:Name="SoDoViTri"
                                                    Margin="0,5,0,0"
                                                    Background="#4c5bd4"
                                                    Cursor="Hand"
                                                    MouseLeftButtonUp="SoDoViTri_MouseLeftButtonUp">
                                                    <DockPanel>
                                                        <Path
                                                            VerticalAlignment="Center"
                                                            Data="{DynamicResource iocnGach}"
                                                            Stroke="#FFFFFF"
                                                            StrokeThickness="2" />
                                                        <TextBlock
                                                            Margin="10,0,0,0"
                                                            VerticalAlignment="Center"
                                                            FontSize="15"
                                                            FontWeight="Regular"
                                                            Foreground="#FFFFFF"
                                                            Text="Thiết lập vị trí" />
                                                    </DockPanel>
                                                </Border>
                                            </StackPanel>
                                        </StackPanel>
                                    </Border>
                                    <Border
                                        x:Name="bod_QuanLyNhanVien"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="bod_QuanLyNhanVien_MouseLeftButtonUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center"
                                            Background="Transparent"
                                            Cursor="Hand">
                                            <Grid>
                                                <Ellipse
                                                    Width="9"
                                                    Height="9"
                                                    Margin="0,0,5,13"
                                                    Fill="#FFFFFF"
                                                    StrokeThickness="1.5" />
                                                <Ellipse
                                                    Width="6"
                                                    Height="6"
                                                    Margin="11,0,0,9"
                                                    Fill="#FFFFFF"
                                                    StrokeThickness="1.5" />
                                                <Path
                                                    Height="24"
                                                    Data="{StaticResource iconNhomNguoiNV}"
                                                    Fill="#FFFFFF"
                                                    StrokeThickness="1.5" />
                                            </Grid>
                                            <TextBlock
                                                x:Name="tb_QuanLyNhanVien"
                                                Margin="10,5,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Quản lý nhân viên" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_CaiDatDeXuat"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="btn_CaiDatDeXuat_MouseLeftButtonUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path Data="{DynamicResource iconCDThoiGianDuyetDX}" Fill="#ffffff" />
                                            <TextBlock
                                                x:Name="tb_Caidatdexuat"
                                                Margin="10,4,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Cài đặt đề xuất" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_GioiHanIp"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseUp="bod_GioiHanIpNPhanMem_MouseUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconIPPM}"
                                                Stroke="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                x:Name="tb_GioiHanIPvaPhanMem"
                                                Margin="10,4,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Giới hạn IP + Phần mềm" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_LuanChuyenCongTy"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="btn_LuanChuyenCongTy_MouseLeftButtonUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconDoiLoaiTK}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                x:Name="tb_LuanChuyenCongTy"
                                                Margin="10,0,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Luân chuyển công ty" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btnPhanQuyen"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="btnPhanQuyen_MouseLeftButtonUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <StackPanel>
                                                <Path
                                                    Data="{DynamicResource iconQuyenTC}"
                                                    Fill="#ffffff"
                                                    StrokeThickness="2" />
                                                <Border
                                                    Width="6"
                                                    Height="6"
                                                    Margin="0,-1,5,0"
                                                    BorderBrush="#ffffff"
                                                    BorderThickness="1"
                                                    CornerRadius="1000" />
                                            </StackPanel>
                                            <TextBlock
                                                x:Name="tb_PhanQuyenHeThong"
                                                Margin="10,0,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Phân quyền hệ thống" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_LichLamViec"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseUp="bod_LichCaLamViec_MouseUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconLichCa}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                x:Name="tb_LichVaCaLamViec"
                                                Margin="10,2,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Lịch làm việc + ca làm việc" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_CaIDatViTriWifi"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconCaiDatViTriWf}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                Margin="10,2,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Cài đặt vị trí và Wifi" />
                                        </DockPanel>
                                    </Border>

                                    <Border x:Name="btn_CheDoChamCong" />

                                    <Border
                                        x:Name="bod_CheDoPheDuyetChamCong"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseUp="bod_CheDoPheDuyetChamCong_MouseUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconCheDoPheDuyetCC}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                x:Name="tb_CheDoPheDuyetChamCong"
                                                Margin="10,2,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Chế độ phê duyệt chấm công" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_TachDongChamCong"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseUp="btn_TachDongChamCong_MouseUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconTachhDongCC}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                x:Name="tb_TachDongChamCongNS"
                                                Margin="10,3,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Tách dòng chấm công của nhân sự" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btnChotDon"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="btnChotDon_MouseLeftButtonUp">
                                        <DockPanel
                                            Margin="11,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconChotDonTu}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                x:Name="tb_ChotDonTu"
                                                Margin="10,5,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Chốt đơn từ" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_NgayChotTuDong"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconChotCongTD}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                Margin="10,3,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Ngày chốt tự động công sau kỳ công" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_HeSoLamTron"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconHSLamTron}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                Margin="10,0,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Hệ số làm tròn" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_ThongTinTaiKhoan"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="btn_ThongTinTaiKhoan_MouseLeftButtonUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconThongTinTK}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                x:Name="tb_ThongTinTaiKhoan"
                                                Margin="10,5,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Thông tin tài khoản" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_BaoLoi"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="btn_BaoLoi_MouseLeftButtonUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                Data="{DynamicResource iconWarning}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                            <TextBlock
                                                x:Name="tb_BaoLoi"
                                                Margin="10,5,0,0"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#ffffff"
                                                Text="Báo lỗi" />
                                        </DockPanel>
                                    </Border>
                                </StackPanel>
                                <StackPanel
                                    x:Name="pnlCollapseSizebar"
                                    Grid.Column="1"
                                    Width="82"
                                    HorizontalAlignment="Left"
                                    Background="#4c5bd4"
                                    Visibility="Visible">
                                    <Border Height="87">
                                        <Image
                                            Height="43"
                                            HorizontalAlignment="Center"
                                            VerticalAlignment="Center"
                                            Source="/Resource/image/Group 632585 (1).png" />
                                    </Border>
                                    <Border
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="#4AA7FF"
                                        CornerRadius="10"
                                        Visibility="Collapsed">
                                        <DockPanel
                                            Margin="15,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path Data="{DynamicResource iconUd}" Fill="#FFFF" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_IconThietLapCongTy"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="bod_CoCau_ViTri_ToChuc_MouseLeftButtonUp">
                                        <DockPanel
                                            Margin="12,0,0,0"
                                            HorizontalAlignment="Left"
                                            VerticalAlignment="Center">
                                            <Path
                                                x:Name="icon_Fill_ThietLapCongTy"
                                                Data="{DynamicResource iconQLCongTy}"
                                                Fill="#ffffff" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_IconQuanLyNhanVien"
                                        Height="51"
                                        Margin="15,0,15,0"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="bod_QuanLyNhanVien_MouseLeftButtonUp">
                                        <Grid>
                                            <Ellipse
                                                x:Name="icon_Fill_QuanLyNhanVien1"
                                                Width="9"
                                                Height="9"
                                                Margin="0,0,5,13"
                                                Fill="#FFFFFF"
                                                StrokeThickness="1.5" />
                                            <Ellipse
                                                x:Name="icon_Fill_QuanLyNhanVien2"
                                                Width="6"
                                                Height="6"
                                                Margin="11,0,0,9"
                                                Fill="#FFFFFF"
                                                StrokeThickness="1.5" />
                                            <Path
                                                x:Name="icon_Fill_QuanLyNhanVien3"
                                                Height="24"
                                                Margin="13,0,0,0"
                                                Data="{StaticResource iconNhomNguoiNV}"
                                                Fill="#FFFFFF"
                                                StrokeThickness="1.5" />
                                        </Grid>
                                    </Border>
                                    <Border
                                        x:Name="btn_IconCaiDatDeXuat"
                                        Height="51"
                                        HorizontalAlignment="Center"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="btn_CaiDatDeXuat_MouseLeftButtonUp">
                                        <DockPanel HorizontalAlignment="Left" VerticalAlignment="Center">
                                            <Path
                                                x:Name="icon_Fill_CaiDatDeXuat"
                                                Margin="15,0,15,0"
                                                Data="{DynamicResource iconCDThoiGianDuyetDX}"
                                                Fill="#ffffff" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_IconGIoiHanIPPhanMem"
                                        Width="51"
                                        Height="51"
                                        HorizontalAlignment="Center"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseUp="bod_GioiHanIpNPhanMem_MouseUp">
                                        <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <Path
                                                x:Name="icon_Fill_GioiHanIpPhanMem"
                                                Data="{DynamicResource iconIPPM}"
                                                Stroke="#ffffff"
                                                StrokeThickness="2" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_IconLuanChuyenCongTy"
                                        Height="51"
                                        HorizontalAlignment="Center"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseLeftButtonUp="btn_LuanChuyenCongTy_MouseLeftButtonUp">
                                        <DockPanel HorizontalAlignment="Left" VerticalAlignment="Center">
                                            <Path
                                                x:Name="icon_Fill_LuanChuyenCongTy"
                                                Margin="15,0,15,0"
                                                Data="{DynamicResource iconDoiLoaiTK}"
                                                Fill="#ffffff"
                                                StrokeThickness="2" />
                                        </DockPanel>
                                    </Border>
                                    <Border
                                        x:Name="btn_IconPhanQuyenHeThong"
                                        Width="51"
                                        Height="51"
                                        HorizontalAlignment="Center"
                                        Background="Transparent"
                                        CornerRadius="10"
                                        MouseUp="btnPhanQuyen_MouseLeftButtonUp">
                                        <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <StackPanel>
