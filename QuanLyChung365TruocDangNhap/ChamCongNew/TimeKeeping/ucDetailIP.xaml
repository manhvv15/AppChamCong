<UserControl x:Name="useControl" x:Class="QuanLyChung365TruocDangNhap.ChamCongNew.TimeKeeping.ucDetailIP"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuanLyChung365TruocDangNhap.ChamCongNew.TimeKeeping"
             mc:Ignorable="d" Width="1200"
             
             >
    <UserControl.Resources>
        <Style x:Key="MyTextBlockStyle" TargetType="TextBlock">
            <Setter Property="Foreground" Value="#474747" />
            <Setter Property="FontWeight" Value="DemiBold" />
            <Setter Property="FontSize" Value="18" />
            <!-- Các thuộc tính khác có thể được thêm vào đây -->
        </Style>
        <Style x:Key="MyBorderStyle" TargetType="Border">
            <Setter Property="BorderBrush" Value="#a9a9a9" />
            <Setter Property="BorderThickness" Value="1" />
            <Setter Property="Padding" Value="7" />
            <Setter Property="CornerRadius" Value="10" />
            <!-- Các thuộc tính khác có thể được thêm vào đây -->
        </Style>
        <Style x:Key="BorList" TargetType="Border">
            <Setter Property="BorderBrush" Value="#a9a9a9" />
            <Setter Property="BorderThickness" Value="1" />
            <Setter Property="Height" Value="190" />

            <Setter Property="CornerRadius" Value="10" />
            <!-- Các thuộc tính khác có thể được thêm vào đây -->
        </Style>
    </UserControl.Resources>

    <Grid >
        <Popup
     x:Name="popUpViewDetailPhongBan"
     Margin="0,0,0,10"
     AllowsTransparency="True"
     IsOpen="False"
     Placement="MousePoint">

            <Border
         MinWidth="100"
         MinHeight="40"
         MaxWidth="800"
         MaxHeight="400"
         Margin="3"
         Padding="10"
         Panel.ZIndex="2"
         Background="White"
         CornerRadius="15">
                <Border.Effect>
                    <DropShadowEffect BlurRadius="16" ShadowDepth="5" />
                </Border.Effect>
                <ScrollViewer HorizontalScrollBarVisibility="Disabled">

                    <ListView
                 x:Name="lsvShowDetaiPhongBan"
                 BorderThickness="0"
                 ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                        <ListView.ItemsPanel>
                            <ItemsPanelTemplate>
                                <WrapPanel />
                            </ItemsPanelTemplate>
                        </ListView.ItemsPanel>
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding organizeDetailName}" FontSize="16"/>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </ScrollViewer>
            </Border>
        </Popup>
        <Popup
x:Name="popUpViewDetailChucVu"
Margin="0,0,0,10"
AllowsTransparency="True"
IsOpen="False"
Placement="MousePoint">

            <Border
    MinWidth="100"
    MinHeight="40"
    MaxWidth="800"
    MaxHeight="400"
    Margin="3"
    Padding="10"
    Panel.ZIndex="2"
    Background="White"
    CornerRadius="15">
                <Border.Effect>
                    <DropShadowEffect BlurRadius="16" ShadowDepth="5" />
                </Border.Effect>
                <ScrollViewer HorizontalScrollBarVisibility="Disabled">

                    <ListView
            x:Name="lsvShowDetaiChucVu"
            BorderThickness="0"
            ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                        <ListView.ItemsPanel>
                            <ItemsPanelTemplate>
                                <StackPanel />
                            </ItemsPanelTemplate>
                        </ListView.ItemsPanel>
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <TextBlock Text="{Binding positionName}" FontSize="16"/>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </ScrollViewer>
            </Border>
        </Popup>
        <Popup
x:Name="popUpViewDetailName"
Margin="0,0,0,10"
AllowsTransparency="True"
IsOpen="False"
Placement="MousePoint">

            <Border
    MinWidth="100"
    MinHeight="40"
    MaxWidth="800"
    MaxHeight="400"
    Margin="3"
    Padding="10"
    Panel.ZIndex="2"
    Background="White"
    CornerRadius="15">
                <Border.Effect>
                    <DropShadowEffect BlurRadius="16" ShadowDepth="5" />
                </Border.Effect>
                <ScrollViewer HorizontalScrollBarVisibility="Disabled">

                    <ListView
            x:Name="lsvShowDetaiName" 
            BorderThickness="0"
            ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                        <ListView.ItemsPanel>
                            <ItemsPanelTemplate>
                                <StackPanel />
                            </ItemsPanelTemplate>
                        </ListView.ItemsPanel>
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <DockPanel>
                                    <TextBlock Text="{Binding userName}" FontSize="16"/>
                                    <TextBlock Text=" - " FontSize="16"/>
                                    <TextBlock Text="{Binding _id}" FontSize="16"/>
                                </DockPanel>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </ScrollViewer>
            </Border>
        </Popup>
        <Popup
x:Name="popUpViewDetailCaLv"
Margin="0,0,0,10"
AllowsTransparency="True"
IsOpen="False"
Placement="MousePoint">

            <Border
    MinWidth="100"
    MinHeight="40"
    MaxWidth="800"
    MaxHeight="400"
    Margin="3"
    Padding="10"
    Panel.ZIndex="2"
    Background="White"
    CornerRadius="15">
                <Border.Effect>
                    <DropShadowEffect BlurRadius="16" ShadowDepth="5" />
                </Border.Effect>
                <ScrollViewer HorizontalScrollBarVisibility="Disabled">

                    <ListView
            x:Name="lsvShowDetaiCaLV"
            BorderThickness="0"
            ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                        <ListView.ItemsPanel>
                            <ItemsPanelTemplate>
                                <StackPanel />
                            </ItemsPanelTemplate>
                        </ListView.ItemsPanel>
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <DockPanel>
                                    <TextBlock Text="{Binding shift_name}" FontSize="16"/>
                                    <TextBlock Text=" - " FontSize="16"/>
                                    <TextBlock Text="{Binding type_name}" FontSize="16"/>
                                </DockPanel>

                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </ScrollViewer>
            </Border>
        </Popup>
        <Popup
x:Name="popUpViewDetailLocation"
Margin="0,0,0,10"
AllowsTransparency="True"
IsOpen="False"
Placement="MousePoint">

            <Border
    MinWidth="100"
    MinHeight="40"
    MaxWidth="800"
    MaxHeight="400"
    Margin="3"
    Padding="10"
    Panel.ZIndex="2"
    Background="White"
    CornerRadius="15">
                <Border.Effect>
                    <DropShadowEffect BlurRadius="16" ShadowDepth="5" />
                </Border.Effect>
                <ScrollViewer HorizontalScrollBarVisibility="Disabled">

                    <ListView
            x:Name="lsvShowDetaiLocation"
            BorderThickness="0"
            ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                        <ListView.ItemsPanel>
                            <ItemsPanelTemplate>
                                <StackPanel />
                            </ItemsPanelTemplate>
                        </ListView.ItemsPanel>
                        <ListView.ItemTemplate>
                            <DataTemplate>
                                <DockPanel>
                                    <TextBlock Text="{Binding cor_location_name}" FontSize="16"/>

                                </DockPanel>

                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </ScrollViewer>
            </Border>
        </Popup>
        <Popup
x:Name="popUpViewDetailWifi"
Margin="0,0,0,10"
AllowsTransparency="True"
IsOpen="False"
Placement="MousePoint">

            <Border
    MinWidth="100"
    MinHeight="40"
    MaxWidth="800"
    MaxHeight="400"
    Margin="3"
    Padding="10"
    Panel.ZIndex="2"
    Background="White"
    CornerRadius="15">
                <Border.Effect>
                    <DropShadowEffect BlurRadius="16" ShadowDepth="5" />
                </Border.Effect>
                <ScrollViewer HorizontalScrollBarVisibility="Disabled">

                    <ListView
            x:Name="lsvShowDetaiWifi"
            BorderThickness="0"
            ScrollViewer.HorizontalScrollBarVisibility="Disabled">
                        <ListView.ItemsPanel>
                            <ItemsPanelTemplate>
                                <StackPanel />
                            </ItemsPanelTemplate>
                        </ListView.ItemsPanel>
                        <ListView.ItemTemplate>
                            <DataTemplate>

                                <TextBlock Text="{Binding name_wifi}" FontSize="16"/>
                            </DataTemplate>
                        </ListView.ItemTemplate>
                    </ListView>
                </ScrollViewer>
            </Border>
        </Popup>
        <StackPanel Background="WhiteSmoke" >
            <Grid x:Name="gridApDungRow0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*" ></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <StackPanel Grid.Column="0" Margin="10 0 0 0">
                    <TextBlock Text="Tên cài đặt" Style="{StaticResource MyTextBlockStyle}"/>
                    <Border Style="{StaticResource MyBorderStyle}">
                        <Grid>
                            <TextBox x:Name="textNhapTenCaiDat" Background="Transparent" VerticalAlignment="Center" FontSize="15"  Foreground="{DynamicResource #474747}" BorderThickness="0" Margin="10 0 0 0" Grid.ColumnSpan="3"/>
                            <TextBlock FontSize="16" Foreground="{DynamicResource #666666}" TextWrapping="Wrap" Text="Tên cài đặt"  IsHitTestVisible="False" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0" Grid.ColumnSpan="3">
                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding ElementName=textNhapTenCaiDat, Path=Text}" Value="">
                                                <Setter Property="Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                        </Grid>
                    </Border>

                </StackPanel>
                <StackPanel Grid.Column="1" Margin="10 0 0 0">
                    <TextBlock Text="Tổ chức" Style="{StaticResource MyTextBlockStyle}"/>
                    <Border Background="Transparent" x:Name="borToChuc1" Style="{StaticResource MyBorderStyle}"  MouseLeftButtonUp="borToChuc1_MouseLeftButtonUp">
                        <Grid Cursor="Hand" Background="Transparent"  >
                            <TextBox x:Name="textNhapTenToChuc" Background="Transparent" TextChanged="textNhapTenToChuc_TextChanged" VerticalAlignment="Center" FontSize="15"  Foreground="{DynamicResource #474747}" BorderThickness="0" Margin="10 0 0 0" Grid.ColumnSpan="3"/>
                            <TextBlock FontSize="16" Foreground="{DynamicResource #666666}" x:Name="textNhapTenToChuc1" TextWrapping="Wrap" Text="Tìm theo tổ chức"  IsHitTestVisible="False" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0" Grid.ColumnSpan="3">
                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding ElementName=textNhapTenToChuc, Path=Text}" Value="">
                                                <Setter Property="Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                            <Image Margin="0 0 10 0" Source="{StaticResource Frame_635607DrawingImageKey}" Width="10" HorizontalAlignment="Right"/>
                        </Grid>
                    </Border>
                </StackPanel>
                <StackPanel Grid.Column="2" Margin="10 0 0 0">
                    <TextBlock Text="Chức vụ" Style="{StaticResource MyTextBlockStyle}"/>
                    <Grid MouseLeftButtonUp="Grid_MouseLeftButtonUp" Cursor="Hand" Background="Transparent">
                        <Border  Style="{StaticResource MyBorderStyle}" >
                            <WrapPanel VerticalAlignment="Center" >
                                <ListView HorizontalAlignment="Left"  x:Name="lsvChucVuAdd" VerticalAlignment="Center" Background="WhiteSmoke" ScrollViewer.HorizontalScrollBarVisibility="Disabled"  BorderThickness="0" ItemsSource="{Binding ListXet}"  Margin="10 0 0 0" Visibility="Collapsed"   >
                                    <ListView.ItemTemplate>
                                        <DataTemplate>
                                            <WrapPanel Background="#dcdcdc" >
                                                <DockPanel>
                                                    <TextBlock   Text="{Binding positionName}" FontSize="16" Foreground="#474747" VerticalAlignment="Center"/>
                                                    <Border x:Name="xoaChucVu" Background="#dcdcdc" Cursor="Hand" MouseEnter="xoaChucVu_MouseEnter" MouseLeave="xoaChucVu_MouseLeave" PreviewMouseLeftButtonUp="xoaChucVu_PreviewMouseLeftButtonUp" >
                                                        <Image  Source="{StaticResource Group_1000007307DrawingImageKey}"  Margin="10 0 0 0" Width="8"  />
                                                    </Border>

                                                </DockPanel>
                                            </WrapPanel>

                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <WrapPanel  x:Name="listwith" Orientation="Horizontal" ></WrapPanel>
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>
                                </ListView>
                                <Grid x:Name="TimchucVu" >
                                    <TextBox x:Name="textNhapViTri" Background="Transparent" TextChanged="textNhapViTri_TextChanged" VerticalAlignment="Center" FontSize="15"  Foreground="{DynamicResource #474747}" BorderThickness="0" Margin="10 0 0 0" Grid.ColumnSpan="3"/>
                                    <TextBlock FontSize="16" x:Name="textNhapViTri1" Foreground="{DynamicResource #666666}" TextWrapping="Wrap" Text="Tìm theo vị trí"  IsHitTestVisible="False" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0" Grid.ColumnSpan="3">
                                        <TextBlock.Style>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="Visibility" Value="Collapsed"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=textNhapViTri, Path=Text}" Value="">
                                                        <Setter Property="Visibility" Value="Visible"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </Grid>
                            </WrapPanel>
                        </Border>
                        <Image  HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0 0 10 0" Source="{StaticResource Frame_635607DrawingImageKey}" Width="10" RenderTransformOrigin="0.5,0.848"/>
                    </Grid>
                </StackPanel>
                <StackPanel Grid.Column="3" Margin="10 0 0 0">
                    <TextBlock Text="Họ và tên" Style="{StaticResource MyTextBlockStyle}"/>
                    <Grid Cursor="Hand" MouseLeftButtonUp="Border_MouseLeftButtonUp_1" Background="Transparent" >
                        <Border Style="{StaticResource MyBorderStyle}" >
                            <WrapPanel VerticalAlignment="Center" >
                                <ListView HorizontalAlignment="Left"  x:Name="lsvNameAdd" VerticalAlignment="Center" Background="WhiteSmoke" ScrollViewer.HorizontalScrollBarVisibility="Disabled"  BorderThickness="0" ItemsSource="{Binding ListXet}"  Margin="10 0 0 0" Visibility="Collapsed"   >
                                    <ListView.ItemTemplate>
                                        <DataTemplate>
                                            <WrapPanel Background="#dcdcdc" >
                                                <DockPanel>
                                                    <TextBlock   Text="{Binding userName}" FontSize="16" Foreground="#474747" VerticalAlignment="Center"/>
                                                    <TextBlock   Text=" - " FontSize="16" Foreground="#474747" VerticalAlignment="Center"/>
                                                    <TextBlock   Text="{Binding idQLC}" FontSize="16" Foreground="#474747" VerticalAlignment="Center"/>
                                                    <Border x:Name="xoaName" Background="#dcdcdc" Cursor="Hand" MouseEnter="xoaName_MouseEnter" MouseLeave="xoaName_MouseLeave" PreviewMouseLeftButtonUp="xoaName_PreviewMouseLeftButtonUp" >
                                                        <Image  Source="{StaticResource Group_1000007307DrawingImageKey}"  Margin="10 0 0 0" Width="8"  />
                                                    </Border>

                                                </DockPanel>
                                            </WrapPanel>

                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <WrapPanel  x:Name="listwith" Orientation="Horizontal" ></WrapPanel>
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>
                                </ListView>

                                <Grid x:Name="gridName">
                                    <TextBox x:Name="textNhapHoVaTen" TextChanged="textNhapHoVaTen_TextChanged" Background="Transparent" VerticalAlignment="Center" FontSize="15"  Foreground="{DynamicResource #474747}" BorderThickness="0" Margin="10 0 0 0" Grid.ColumnSpan="3"/>
                                    <TextBlock x:Name="textNhapHoVaTen1" FontSize="16" Foreground="{DynamicResource #666666}" TextWrapping="Wrap" Text="Tìm theo id nhân viên"  IsHitTestVisible="False" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0" Grid.ColumnSpan="3">
                                        <TextBlock.Style>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="Visibility" Value="Collapsed"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=textNhapHoVaTen, Path=Text}" Value="">
                                                        <Setter Property="Visibility" Value="Visible"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </Grid>
                            </WrapPanel>
                        </Border>
                        <Image Margin="0 0 10 0" Source="{StaticResource Frame_635607DrawingImageKey}" Width="10" HorizontalAlignment="Right"/>
                    </Grid>
                </StackPanel>
            </Grid>
            <Grid x:Name="gridApDungRow1" Margin="0 20 0 0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*" ></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <StackPanel Grid.Column="0" Margin="10 0 0 0">
                    <TextBlock Text="Thời gian bắt đầu" Style="{StaticResource MyTextBlockStyle}"/>
                    <Border CornerRadius="10"   BorderThickness="1" BorderBrush="{DynamicResource #aaa}" Height="40"   Visibility="Visible" Grid.ColumnSpan="2">

                        <DatePicker x:Name="NgayBatDau" Style="{StaticResource DatePicker11}" FontSize="15" />
                    </Border>
                </StackPanel>
                <StackPanel Grid.Column="1" Margin="10 0 0 0">
                    <TextBlock Text="Thời gian kết thúc" Style="{StaticResource MyTextBlockStyle}"/>
                    <Border CornerRadius="10" BorderThickness="1" BorderBrush="{DynamicResource #aaa}" Height="40"  Visibility="Visible" Grid.ColumnSpan="2">

                        <DatePicker x:Name="NgayKetThuc" Style="{StaticResource DatePicker11}" FontSize="15" />
                    </Border>
                </StackPanel>
                <StackPanel Grid.Column="2" Margin="10 0 0 0">
                    <TextBlock Text="Ca làm việc" Style="{StaticResource MyTextBlockStyle}"/>
                    <Grid Background="Transparent" Cursor="Hand" MouseDown="Grid_MouseLeftButtonUp_1">
                        <Border Style="{StaticResource MyBorderStyle}">
                            <WrapPanel VerticalAlignment="Center" >
                                <ListView HorizontalAlignment="Left"  x:Name="lsvCaAdd" VerticalAlignment="Center" Background="WhiteSmoke" ScrollViewer.HorizontalScrollBarVisibility="Disabled"  BorderThickness="0" ItemsSource="{Binding ListXet}"  Margin="10 0 0 0" Visibility="Collapsed"   >
                                    <ListView.ItemTemplate>
                                        <DataTemplate>
                                            <WrapPanel Background="#dcdcdc" >
                                                <DockPanel>
                                                    <TextBlock   Text="{Binding shift_name}" FontSize="16" Foreground="#474747" VerticalAlignment="Center"/>
                                                    <TextBlock   Text=" - " FontSize="16" Foreground="#474747" VerticalAlignment="Center"/>
                                                    <TextBlock   Text="{Binding type_name}" FontSize="16" Foreground="#474747" VerticalAlignment="Center"/>
                                                    <Border x:Name="xoaCa" Background="#dcdcdc" Cursor="Hand" MouseEnter="Ca_MouseEnter" MouseLeave="Ca_MouseLeave" PreviewMouseLeftButtonUp="Ca_PreviewMouseLeftButtonUp" >
                                                        <Image  Source="{StaticResource Group_1000007307DrawingImageKey}"  Margin="10 0 0 0" Width="8"  />
                                                    </Border>

                                                </DockPanel>
                                            </WrapPanel>

                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <WrapPanel  x:Name="listwith" Orientation="Horizontal" ></WrapPanel>
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>
                                </ListView>

                                <Grid x:Name="gridca">
                                    <TextBox x:Name="textNhapCa" TextChanged="textNhapCa_TextChanged" Background="Transparent"  VerticalAlignment="Center" FontSize="15"  Foreground="{DynamicResource #474747}" BorderThickness="0" Margin="10 0 0 0" Grid.ColumnSpan="3"/>
                                    <TextBlock FontSize="16" x:Name="textNhapCa1" Foreground="{DynamicResource #666666}" TextWrapping="Wrap" Text="Tìm theo ca làm việc"  IsHitTestVisible="False" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0" Grid.ColumnSpan="3">
                                        <TextBlock.Style>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="Visibility" Value="Collapsed"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=textNhapCa, Path=Text}" Value="">
                                                        <Setter Property="Visibility" Value="Visible"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </Grid>
                            </WrapPanel>
                        </Border>
                        <Image Margin="0 0 10 0" Source="{StaticResource Frame_635607DrawingImageKey}" Width="10" HorizontalAlignment="Right"/>
                    </Grid>
                </StackPanel>
                <StackPanel Grid.Column="3" Margin="10 0 0 0">
                    <TextBlock Text="Vị trí" Style="{StaticResource MyTextBlockStyle}"/>
                    <Grid Cursor="Hand" Background="Transparent" MouseDown="Grid_OpenPopupLoc_MouseLeftButtonUp">
                        <Border Style="{StaticResource MyBorderStyle}">

                            <WrapPanel VerticalAlignment="Center" >
                                <ListView HorizontalAlignment="Left"  x:Name="lsvLocAdd" VerticalAlignment="Center" Background="WhiteSmoke" ScrollViewer.HorizontalScrollBarVisibility="Disabled"  BorderThickness="0" ItemsSource="{Binding ListXet}"  Margin="10 0 0 0" Visibility="Collapsed"   >
                                    <ListView.ItemTemplate>
                                        <DataTemplate>
                                            <WrapPanel Width="180" Background="#dcdcdc" >
                                                <DockPanel Width="180"  Margin="0 0 0 0">
                                                    <TextBlock Width="150" Margin="0 0 0 0" TextTrimming="CharacterEllipsis"   Text="{Binding cor_location_name}" FontSize="16" Foreground="#474747" VerticalAlignment="Center"/>
                                                    <Border x:Name="xoaLoc" Background="#dcdcdc" Cursor="Hand" MouseEnter="Loc_MouseEnter" MouseLeave="Loc_MouseLeave" PreviewMouseLeftButtonUp="Loc_PreviewMouseLeftButtonUp" >
                                                        <Image  Source="{StaticResource Group_1000007307DrawingImageKey}"  Margin="5 0 0 0" Width="8"  />
                                                    </Border>

                                                </DockPanel>
                                            </WrapPanel>

                                        </DataTemplate>
                                    </ListView.ItemTemplate>
                                    <ListBox.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <WrapPanel  x:Name="listwith" Orientation="Horizontal" ></WrapPanel>
                                        </ItemsPanelTemplate>
                                    </ListBox.ItemsPanel>
                                </ListView>

                                <Grid x:Name="gridLoc">
                                    <TextBox x:Name="textNhapLoc" TextChanged="textNhapLoc_TextChanged" Background="Transparent" VerticalAlignment="Center" FontSize="15"  Foreground="{DynamicResource #474747}" BorderThickness="0" Margin="10 0 0 0" Grid.ColumnSpan="3"/>
                                    <TextBlock FontSize="16" x:Name="textNhapLoc1" Foreground="{DynamicResource #666666}" TextWrapping="Wrap" Text="Tìm theo vị trí"  IsHitTestVisible="False" VerticalAlignment="Center" Margin="10,0,0,0" HorizontalAlignment="Left" Width="196">
                                        <TextBlock.Style>
                                            <Style TargetType="TextBlock">
                                                <Setter Property="Visibility" Value="Collapsed"/>
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=textNhapLoc, Path=Text}" Value="">
                                                        <Setter Property="Visibility" Value="Visible"/>
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>
                                </Grid>

                            </WrapPanel>
                        </Border>
                        <Image Margin="0 0 10 0" Source="{StaticResource Frame_635607DrawingImageKey}" Width="10" HorizontalAlignment="Right"/>

                    </Grid>
                </StackPanel>
            </Grid>
            <Grid Margin="0 20 0 0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*" ></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                    <ColumnDefinition Width="1*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <StackPanel Grid.Column="0" Margin="10 0 0 0" Visibility="Collapsed">
                    <TextBlock Text="Vị trí" Style="{StaticResource MyTextBlockStyle}"/>
                    <Border Style="{StaticResource MyBorderStyle}">

                        <Grid>
                            <TextBox x:Name="textViTri" Background="Transparent" VerticalAlignment="Center" FontSize="15"  Foreground="{DynamicResource #474747}" BorderThickness="0" Margin="10 0 0 0" Grid.ColumnSpan="3"/>
                            <TextBlock FontSize="16" Foreground="{DynamicResource #666666}" TextWrapping="Wrap" Text="Tìm theo vị trí"  IsHitTestVisible="False" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 0 0 0" Grid.ColumnSpan="3">
                                <TextBlock.Style>
                                    <Style TargetType="TextBlock">
                                        <Setter Property="Visibility" Value="Collapsed"/>
                                        <Style.Triggers>
                                            <DataTrigger Binding="{Binding ElementName=textViTri, Path=Text}" Value="">
                                                <Setter Property="Visibility" Value="Visible"/>
                                            </DataTrigger>
                                        </Style.Triggers>
                                    </Style>
                                </TextBlock.Style>
                            </TextBlock>
                            <Image Margin="0 0 10 0" Source="{StaticResource Frame_635607DrawingImageKey}" Width="10" HorizontalAlignment="Right"/>

                        </Grid>

