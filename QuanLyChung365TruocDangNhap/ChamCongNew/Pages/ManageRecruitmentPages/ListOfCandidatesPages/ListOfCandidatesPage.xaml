                    <Border
                        Margin="0,25,0,0"
                        Padding="5"
                        HorizontalAlignment="Right"
                        VerticalAlignment="Top"
                        Background="Transparent"
                        BorderBrush="#20744A"
                        BorderThickness="1"
                        CornerRadius="4"
                        Cursor="Hand">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="Auto" />
                            </Grid.ColumnDefinitions>
                            <Path
                                Grid.Column="0"
                                Cursor="Hand"
                                Data="{StaticResource icon_excel}"
                                Fill="{StaticResource #20744A}" />
                            <TextBlock
