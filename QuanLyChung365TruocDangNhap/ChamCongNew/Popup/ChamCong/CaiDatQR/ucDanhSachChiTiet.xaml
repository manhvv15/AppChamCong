                                                MouseLeave="Location_Mouselef" />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="auto" Header="Wifi">

                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <Grid>

                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#474747"
                                                Text="{Binding displayDp7}" />
                                            <Grid
                                                Margin="0,0,0,0"
                                                Background="Transparent"
                                                DataContext="{Binding list_wifi}"
                                                MouseEnter="wifi_enter"
                                                MouseLeave="wifi_left" />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="auto" Header="Thiết bị">

                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <Grid>

                                            <TextBlock
                                                HorizontalAlignment="Center"
                                                VerticalAlignment="Center"
                                                FontSize="16"
                                                FontWeight="DemiBold"
                                                Foreground="#474747"
                                                Text="{Binding displayDp5}" />
                                            <Grid
                                                Margin="0,0,0,0"
                                                Background="Transparent"
                                                DataContext="{Binding list_device}"
                                                MouseEnter="Device_MouseEnter"
                                                MouseLeave="Device_Mouselef" />
                                        </Grid>
                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                            <DataGridTemplateColumn Width="auto" Header="Hành động">

                                <DataGridTemplateColumn.CellTemplate>
                                    <DataTemplate>
                                        <DockPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                                            <StackPanel Orientation="Horizontal">
                                                <Grid
                                                    Background="Transparent"
                                                    Cursor="Hand"
                                                    MouseLeftButtonUp="Edit_MouseUp">
                                                    <Path Data="{DynamicResource but1}" Fill="#666666" />
                                                    <Path Data="{DynamicResource but2}" Fill="#666666" />
                                                    <Path Data="{DynamicResource but3}" Fill="#666666" />
                                                </Grid>
                                                <Border
                                                    Margin="10,0,10,0"
                                                    Cursor="Hand"
                                                    MouseLeftButtonUp="Delete_MouseLeftButtonUp">


                                                    <Path Data="{DynamicResource xoaa1}" Fill="#666666" />
                                                </Border>
                                            </StackPanel>
                                        </DockPanel>

                                    </DataTemplate>
                                </DataGridTemplateColumn.CellTemplate>
                            </DataGridTemplateColumn>
                        </DataGrid.Columns>
                    </DataGrid>

                </ScrollViewer>
            </Grid>

        </StackPanel>
