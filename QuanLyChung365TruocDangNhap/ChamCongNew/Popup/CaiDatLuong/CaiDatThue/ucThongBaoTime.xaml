<UserControl x:Class="QuanLyChung365TruocDangNhap.ChamCongNew.Popup.CaiDatLuong.CaiDatThue.ucThongBaoTime"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuanLyChung365TruocDangNhap.ChamCongNew.Popup.CaiDatLuong.CaiDatThue"
             mc:Ignorable="d">
    <Grid>
        <Rectangle x:Name="bor" Fill="#000000" Opacity="0.5" MouseLeftButtonUp="Rectangle_MouseLeftButtonUp"></Rectangle>
        <Border Width="250" Height="95" VerticalAlignment="Top" CornerRadius="10" Background="#FFFFFF">
            <StackPanel Margin="20">
                <TextBlock Text="Bạn chưa thiết lập công thức" FontSize="16" FontWeight="Regular" Foreground="#474747"/>
                <Border x:Name="bod_Close" MouseLeftButtonUp="bod_Close_MouseLeftButtonUp" Style="{DynamicResource BorderButon}" Cursor="Hand"  Margin="0 10 0 0">
                    <TextBlock Text="OK" Margin="20 0 20 0" Foreground="#FFFFFF" FontSize="16" FontWeight="Regular"/>
                </Border>
            </StackPanel>
        </Border>    
    </Grid>
</UserControl>
