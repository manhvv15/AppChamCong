<UserControl x:Class="QuanLyChung365TruocDangNhap.PhanQuyenHeThong.ThongBao.Xoa"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:QuanLyChung365TruocDangNhap.PhanQuyenHeThong.ThongBao"
             mc:Ignorable="d" 
             >
    <Grid x:Name="thanhCong" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0 40 50 0">
        <Border Padding="20" Background="#FFFFFF" CornerRadius="10">
            <DockPanel >
                <Border Width="80">
                    <Path Data="{DynamicResource tichxanh}" Fill="#00CE2D" Width="68"  HorizontalAlignment="Center" VerticalAlignment="Center"></Path>
                </Border>
                <TextBlock Text="Xóa thành công" Foreground="#474747" FontSize="20" VerticalAlignment="Center" Margin="20 0 0 0"></TextBlock>
            </DockPanel>
        </Border>
    </Grid>
</UserControl>
